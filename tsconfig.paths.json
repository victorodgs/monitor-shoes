{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets": ["assets"],
      "@assets/*": ["assets/*"],
      "@components": ["components"],
      "@components/*": ["components/*"],
      "@containers": ["containers"],
      "@containers/*": ["containers/*"],
      "@pages": ["pages"],
      "@pages/*": ["pages/*"],
      "@services": ["services"],
      "@services/*": ["services/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@store": ["store"],
      "@store/*": ["store/*"],
      "@/*": ["*"]
    }
  }
}
